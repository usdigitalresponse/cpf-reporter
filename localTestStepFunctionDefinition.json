{
    "Comment": "Test Run local Lambda Function",
    "StartAt": "RunLocalTestLambda",
    "States": {
        "RunLocalTestLambda" : {
            "Type" : "Task",
            "Resource" : "arn:aws:states:::lambda:invoke",
            "OutputPath" : "$.Payload",
            "Parameters" : {
              "FunctionName" : "arn:aws:lambda:us-west-2:000000000000:function:localstack-lambda-url-example",
              "Payload.$" : "$"
            },
            "Retry" : [
              {
                "ErrorEquals" : [
                  "Lambda.ServiceException",
                  "Lambda.AWSLambdaException",
                  "Lambda.SdkClientException",
                  "Lambda.TooManyRequestsException"
                ],
                "IntervalSeconds" : 1,
                "MaxAttempts" : 3,
                "BackoffRate" : 2
              }
            ],
            "End" : true
        }
    }
}
