generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Agency {
  id             Int           @id @default(autoincrement())
  name           String
  abbreviation   String?
  code           String
  tenant         Organization? @relation(fields: [organizationId], references: [id])
  organizationId Int?
}

model Organization {
  id       Int      @id @default(autoincrement())
  agencies Agency[]
  name     String
}
